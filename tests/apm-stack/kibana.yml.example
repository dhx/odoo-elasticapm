server.host: 0.0.0.0
status.allowAnonymous: true
monitoring.ui.container.elasticsearch.enabled: false
telemetry.enabled: false
xpack.reporting.encryptionKey: __SECRET32CHARS__
xpack.security.encryptionKey: __SECRET32CHARS__
xpack.encryptedSavedObjects.encryptionKey: __SECRET32CHARS__

xpack.fleet.packages:
  - name: fleet_server
    version: latest
  - name: apm
    version: latest
xpack.fleet.agentPolicies:
  - name: Fleet Server (APM)
    id: fleet-server-apm
    is_default_fleet_server: true
    is_managed: false
    namespace: default
    package_policies:
      - name: fleet_server-apm
        id: default-fleet-server
        package:
          name: fleet_server
      - name: apm-server
        id: local-apm-server
        package:
          name: apm
